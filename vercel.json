{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/bot.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["node_modules/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/bot.js"
    }
  ]
}