{
    "version": 2,
    "builds": [
      {
        "src": "api/bot.js",
        "use": "@vercel/node"
      },
      {
        "src": "pages/index.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/bot.js"
      }
    ]
  }
  